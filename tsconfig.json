{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "astro"
    },
    "include": [
        "src/**/*",
        ".astro/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}